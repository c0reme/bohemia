{
  "prettier.configPath": ".prettierrc",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
    "source.fixAll.prettier": true
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "json"
  ],
  "cSpell.words": [
    "crossworld",
    "cwls",
    "Dynamis",
    "headpats",
    "linkshell",
    "Loporrit",
    "Tweetingway",
    "xivapi"
  ]
}
